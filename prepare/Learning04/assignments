using System;

class Assignments
{
    private string _name;
    private string _topic; //"Fractions"


    //build constructor
    //access type, parameters
    public Assignments(string _name, string _topic)
    {
        this._name = _name;
        this._topic = _topic;
        
    }


    //build methods
    
    public string GetSummary()
    {
        // Interpolate a string $""
        return $"{_name} - {_topic}";
    }
}